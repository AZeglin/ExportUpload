    <add key="applicationServerName" value="AMMHINPRDAPP3"/>
    <add key="socketHandlerUrl" value="ws://AMMHINPRDAPP3.vha.med.va.gov//NACCMExportUpload/ExportUploadHttpHandler.ashx" />

    <add key="exportUploadDatabaseServerName" value="AMMHINPRDDB3"/>
